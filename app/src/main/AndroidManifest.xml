            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.a20203000.memo.ModifyActivity"/>
        <activity android:name="com.a20203000.memo.newmemo"/>

